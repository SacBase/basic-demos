Module useTypesMod:

import List: all;
import Random: all;
import StdIO: all;

list newList()
{
  return(nil());
}

list appendList(list l1, list l2)
{
  return(append(l1, l2));
}

list fillList(list l, int count)
{
  int i;
  for(i=1; i<=count; i++) {
    l = cons(random(1, 100), l);
  }
  return(l);
}

void printList(list l)
{
  printf("list: <");
  while(length(l)>0) {
    printf( "%d", hd(l));
    printf( " ");
    l = drop( 1, l);
  } 
  printf(">\n");
}


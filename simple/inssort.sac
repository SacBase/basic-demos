use Array: all;
use SimplePrint: all;

int[.] inssort(int[.] A) {
 n = shape(A)[0];

 for (i = 1; i < n; i++) {
   t = A[i];
   j = i;
   while ((j > 0) && (A[j-1] > t)) {
     A[j] = A[j-1];
     j--;
   }
   A[j] = t;
 }

 return( A);
}

int main() {
  A = with ( . <= iv = [i] <= .)
      genarray( [16], 16-i, 0);
  c = print( A);

  B = inssort( A);
  c += print( B);

  return(c);
}

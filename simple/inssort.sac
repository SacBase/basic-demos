use Array: all;
use StdIO: {print};

int[.] inssort(int[.] A) {
 n = shape(A)[0];

 for (i = 1; i < n; i++) {
   t = A[i];
   j = i;
   while ((j > 0) && (A[j-1] > t)) {
     A[j] = A[j-1];
     j--;
   }
   A[j] = t;
 }

 return( A);
}

int main() {
  
  A = with {
        ( . <= iv = [i] <= . ) : 16-i;
      } : genarray( [16], 0 );

  print( A);
  B = inssort( A);
  print( B);

  return(0);
}

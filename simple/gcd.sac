use SimplePrint : all; 
use ScalarArith: all;
 
     
int gcd (int high, int low) 
{
  if (high < low) {
    mem = low;
    low = high;
    high = mem;
  }
  while (low != 0) {
    quotient, remainder = modulo (high,low);
    high = low;
    low = remainder;
  }
return (high);
}
 
int, int modulo (int x, int y)
{ 
  quot = x/y;
  remain = x - quot * y;
  return (quot,remain);
} 
 
int main ()
{
  res = print( gcd(22, 27));
  return (res);
}




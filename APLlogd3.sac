import StdIO: all;
import Array: all;

#define NN 5000000


inline double[] diff3( double[] x)
{
  res = ( x - shift( 0, 1, 0d, x));
  return( res);
}

double[] logd3( double[] a)
{
  res = max( -50d, min( 50d, 50d*(diff3(  a) / (0.01+a))));
  return( res);
}

int main()
{
  a = with( . <= iv <= .)
      genarray( [NN], .5+iv[0] );
  res = sum( logd3( a));
  printf( "result: %f\n", res);
  return(0);
}
